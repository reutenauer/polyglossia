\ProvidesFile{gloss-latex.ldf}[polyglossia: module for default language]

\PolyglossiaSetup{latex}{
  bcp47=en-US,
  bcp47-language=en,
  bcp47-region=US,
  bcp47-script=Latn,
  hyphennames={english},
  hyphenmins={2,3},
  langtag=ENG,
  fontsetup=true,
  % ignore command
  DefineCommandsCmd={use_none:n}
}

